{
    "manifest_version":2,
    
    "name":"Skip",
    
    "description": "Pula paginas",
    
    "version":"0.1",
    
    "browser_action":{
        "name":"Skip",
		"default_popup":"skip.html",
        "default_icon":"icon.png"       		
    },
    
    "content_scripts":[{
        "matches": [ "http://*/*", "https://*/*"],
        "js":["jquery.min.js","skip.js"]
        
    }],
    "permissions":["activeTab","tabs"]
}